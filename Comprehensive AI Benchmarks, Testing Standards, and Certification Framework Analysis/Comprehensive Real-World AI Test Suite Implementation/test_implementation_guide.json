      "onnx>=1.12.0",
      "opencv-python>=4.6.0",
      "scikit-learn>=1.1.0",
      "pandas>=1.4.0",
      "numpy>=1.21.0"
    ],
    "hardware_minimum": {
      "cpu": "8 cores, 3.0 GHz",
      "memory": "32 GB RAM",
      "storage": "500 GB SSD",
      "gpu_optional": "NVIDIA RTX 3080 or equivalent"
    }
  },
  "execution_phases": [
    {
      "phase": "1. Environment Setup",
      "steps": [
