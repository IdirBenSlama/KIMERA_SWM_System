{
  "setup_requirements": {
    "python_version": "3.8+",
    "required_packages": [
      "torch>=1.12.0",
      "tensorflow>=2.8.0",
      "transformers>=4.20.0",
      "datasets>=2.0.0",
      "evaluate>=0.2.0",
